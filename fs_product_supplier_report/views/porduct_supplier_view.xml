<?xml version="1.0" encoding="UTF-8"?>
<odoo>
		<record id="view_product_supplier_form" model="ir.ui.view">
			<field name="name">view.product.supplier.form</field>
			<field name="model">product.supplier</field>
			<field name="arch" type="xml">
				<form string="Report" create="false">
					<sheet>
						<group col="2">
							<group>
								<field name="product" />
								<field name="qte_stock"/>
								<field name="fournisseur"/>
							</group>
							<group>
								
								<field name="date" />
								<field name="qte_achete" />
								<field name="price_unit" />
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="view_product_supplier_tree" model="ir.ui.view">
			<field name="name">view.product.supplier.list</field>
			<field name="model">product.supplier</field>
			<field name="arch" type="xml">
				<tree string="Report" create="false">
					<field name="product" />
					<field name="qte_stock"/>
					<field name="fournisseur"/>
					<field name="date" />
					<field name="qte_achete" />
					<field name="price_unit" />
				</tree>
			</field>
		</record>

		<record id="view_product_supplier_search" model="ir.ui.view">
			<field name="name">view.product.supplier.search</field>
			<field name="model">product.supplier</field>
			<field name="arch" type="xml">
				<search string="Report">
					<field name="product" />
					<field name="qte_stock"/>
					<field name="fournisseur"/>
					<field name="date" />
					<field name="qte_achete" />
					<field name="price_unit" />
				</search>
			</field>
		</record>

</odoo>